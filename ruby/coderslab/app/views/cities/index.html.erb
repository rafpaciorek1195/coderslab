<table>
  <tr>
    <th>ID</th>
    <th>Miasto</th>
    <th>Panstwo</th>
    <th>Stworzona</th>
    <th>Aktualizowana</th>
  </tr>
  <% @cities.each do |r| %>
    <tr>
    <td> <%= r.id %> </td>
    <td> <%= r.city_name %> </td>
    <td> <%= r.city_country %></td>
    <td> <%= r.created_at  %></td> 
    <td> <%= r.updated_at %> </td>
    </tr>
  <% end %>
  </table>